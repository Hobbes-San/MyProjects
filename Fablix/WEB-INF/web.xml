<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app version="3.0"
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<resource-ref>
		<description>
			Resource reference to a factory for java.sql.Connection
			instances that may be used for talking to a particular
			database that
			is configured in the server.xml file.
		</description>
		<res-ref-name>
			jdbc/TestDB
		</res-ref-name>
		<res-type>
			javax.sql.DataSource
		</res-type>
		<res-auth>
			Container
		</res-auth>
	</resource-ref>
  
    <servlet>
		<servlet-name>SearchingServlet</servlet-name>
		<servlet-class>Searching</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LogInServlet</servlet-name>
		<servlet-class>LogIn</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LogOutServlet</servlet-name>
		<servlet-class>LogOut</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ShoppingCartServlet</servlet-name>
		<servlet-class>ShoppingCart</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SingleMovieServlet</servlet-name>
		<servlet-class>SingleMovie</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>SingleStarServlet</servlet-name>
		<servlet-class>SingleStar</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>CheckoutServlet</servlet-name>
		<servlet-class>Checkout</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>Home</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>FullTextSearchServlet</servlet-name>
		<servlet-class>FullTextSearch</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>PopupMovieInfoServlet</servlet-name>
		<servlet-class>PopupMovieInfo</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>dashboardLogInServlet</servlet-name>
		<servlet-class>dashboardLogIn</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>dashboardingServlet</servlet-name>
		<servlet-class>dashboarding</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>InsertMovieServlet</servlet-name>
		<servlet-class>InsertMovie</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>InsertStarServlet</servlet-name>
		<servlet-class>InsertStar</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>LogInAndroidServlet</servlet-name>
		<servlet-class>LogInAndroid</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>SearchingServlet</servlet-name>
		<url-pattern>/servlet/Searching</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LogInServlet</servlet-name>
		<url-pattern>/servlet/LogIn</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LogOutServlet</servlet-name>
		<url-pattern>/servlet/LogOut</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ShoppingCartServlet</servlet-name>
		<url-pattern>/servlet/ShoppingCart</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SingleMovieServlet</servlet-name>
		<url-pattern>/servlet/SingleMovie</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SingleStarServlet</servlet-name>
		<url-pattern>/servlet/SingleStar</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CheckoutServlet</servlet-name>
		<url-pattern>/servlet/Checkout</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/servlet/Home</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>FullTextSearchServlet</servlet-name>
		<url-pattern>/servlet/FullTextSearch</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>PopupMovieInfoServlet</servlet-name>
		<url-pattern>/servlet/PopupMovieInfo</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>dashboardLogInServlet</servlet-name>
		<url-pattern>/servlet/dashboardLogIn</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>dashboardingServlet</servlet-name>
		<url-pattern>/servlet/dashboarding</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>InsertMovieServlet</servlet-name>
		<url-pattern>/servlet/InsertMovie</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>InsertStarServlet</servlet-name>
		<url-pattern>/servlet/InsertStar</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LogInAndroidServlet</servlet-name>
		<url-pattern>/servlet/LogInAndroid</url-pattern>
	</servlet-mapping>

</web-app>
